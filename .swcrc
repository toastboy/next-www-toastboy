{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false
        },
        "transform": {
            "react": {
                "runtime": "automatic",
                "useBuiltins": true,
                "refresh": false
            }
        },
        "target": "es2022",
        "externalHelpers": false
    },
    "module": {
        "type": "commonjs"
    },
    "sourceMaps": true
}
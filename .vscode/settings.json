{
    "testing.automaticallyOpenTestResults": "neverOpen",
    "yaml.schemas": {
        "https://json.schemastore.org/github-action.json": "file:///Users/toastboy/next-www-toastboy/.github/workflows/terraform-plan.yml"
    },
    "jest.outputConfig": {
        "revealOn": "error",
        "revealWithFocus": "test-results",
        "clearOnRun": "both"
    },
    "jest.runMode": {
        "type": "on-demand",
        "runAllTestsOnStartup": false
    },
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true
    },
    "search.exclude": {
        "**/.git": true,
        "**/.next": true,
        "**/build": true,
        "**/coverage": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/playwright-report": true,
        "**/storybook-static": true,
        "**/test-results": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced"
    },
    "vitest.enable": true,
    "vitest.ignoreWorkspace": true,
    "vitest.rootConfig": "vitest.components.config.ts",
    "vitest.shellType": "child_process"
}
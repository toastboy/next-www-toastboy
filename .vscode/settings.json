{
    "testing.automaticallyOpenTestResults": "neverOpen",
    "yaml.schemas": {
        "https://json.schemastore.org/github-action.json": "file:///Users/toastboy/next-www-toastboy/.github/workflows/terraform-plan.yml"
    },
    "git.postCommitCommand": "sync",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true
    },
    "search.exclude": {
        "**/.git": true,
        "**/.next": true,
        "**/build": true,
        "**/coverage": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/playwright-report": true,
        "**/storybook-static": true,
        "**/test-results": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced"
    },
    "[javascript]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[typescript]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[javascriptreact]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[typescriptreact]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "vitest.enable": true,
    "vitest.ignoreWorkspace": true,
    "vitest.rootConfig": "vitest.unit.config.ts",
    "vitest.shellType": "child_process",
    "eslint.useFlatConfig": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "playwright.reuseBrowser": false,
    "playwright.showTrace": false
}
{
  "name": "next-www-toastboy",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next",
    "deepclean": "rm -rf .next node_modules package-lock.json && npm cache clean --force",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch --passWithNoTests",
    "dev": "npx prisma generate && docker compose up -d && next dev",
    "build": "npx prisma generate && DEBUG=app:* next build",
    "lint": "eslint .",
    "start:ci": "next start --hostname 127.0.0.1 --port 3000",
    "importlivedb": "npx ts-node -r tsconfig-paths/register --project tsconfig.scripts.json --compiler-options '{\"module\":\"CommonJS\"}' src/lib/importlivedb/importlivedb.ts",
    "crawllivesite": "npx ts-node -r tsconfig-paths/register --project tsconfig.scripts.json --compiler-options '{\"module\":\"CommonJS\"}' src/lib/crawllivesite/crawllivesite.ts",
    "start": "docker compose up -d && next start",
    "stop": "docker compose down"
  },

{
  "name": "next-www-toastboy",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next",
    "deepclean": "rm -rf .next node_modules package-lock.json && npm cache clean --force",
    "test": "jest",
    "test:watch": "jest --watch --passWithNoTests",
    "dev": "op run --env-file=.env -- sh -c 'npx prisma generate && docker compose up -d && next dev --turbopack'",
    "build": "op run --env-file=.env -- npx prisma generate && DEBUG=app:* next build",
    "lint": "next lint",
    "importlivedb": "pushd src/lib/importlivedb && npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' importlivedb.ts && popd",
    "crawllivesite": "pushd src/lib/crawllivesite && npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' crawllivesite.ts && popd",
    "start": "docker compose up -d && next start",
    "stop": "docker compose down"
  },
  "dependencies": {
    "@azure/identity": "^4.9.0",
    "@azure/storage-blob": "^12.27.0",
    "@mantine/code-highlight": "^8.2.7",
    "@mantine/core": "^8.2.7",
    "@mantine/dates": "^8.2.7",
    "@mantine/form": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
    "@mantine/notifications": "^8.2.7",
    "@mantine/tiptap": "^8.2.7",
    "@prisma/client": "^6.6.0",
    "@sentry/nextjs": "^10.7.0",
    "@sentry/types": "^10.7.0",
    "@tabler/icons-react": "^3.31.0",
    "@tiptap/extension-highlight": "^3.3.0",
    "@tiptap/extension-link": "^3.3.0",
    "@tiptap/extension-placeholder": "^3.3.0",
    "@tiptap/extension-text-align": "^3.3.0",
    "@tiptap/extension-underline": "^3.3.0",
    "@tiptap/pm": "^3.3.0",
    "@tiptap/react": "^3.3.0",
    "@tiptap/starter-kit": "^3.3.0",
    "better-auth": "^1.3.7",
    "d3": "^7.9.0",
    "dayjs": "^1.11.13",
    "next": "^15.3.1",
    "nodemailer": "^7.0.5",
    "prisma": "^6.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "swr": "^2.3.3",
    "zod-prisma-types": "^3.2.4"

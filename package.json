{
  "name": "next-www-toastboy",
  "private": true,
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --passWithNoTests",
    "dev": "docker compose up -d && next dev",
    "build": "npx prisma generate && DEBUG=app:* next build",
    "lint": "next lint",
    "importlivedb": "pushd src/lib/importlivedb && npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' importlivedb.ts && popd",
    "crawllivesite": "pushd src/lib/crawllivesite && npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' crawllivesite.ts && popd",
    "start": "docker compose up -d && next start",
    "stop": "docker compose down"
  },
  "dependencies": {
    "@azure/identity": "^4.4.1",
    "@azure/storage-blob": "^12.25.0",
    "@mantine/core": "^7.13.0",
    "@mantine/dates": "^7.13.0",
    "@mantine/hooks": "^7.13.0",
    "@prisma/client": "^5.20.0",
    "@stackframe/stack": "^2.6.7",
    "@tabler/icons-react": "^3.19.0",
    "next": "^14.2.13",
    "prisma": "^5.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "swr": "^2.2.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@jest/globals": "^29.7.0",
    "@next/eslint-plugin-next": "^14.2.13",
    "@playwright/test": "^1.47.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",

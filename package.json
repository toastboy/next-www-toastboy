    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.13",
    "eslint-plugin-react": "^7.37.0",
    "glob": "^11.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "jest-mock-extended": "^3.0.7",
    "postcss": "^8.4.47",

    "@eslint/object-schema": "^2.1.6",
    "@jest/globals": "^30.1.1",
    "@next/eslint-plugin-next": "^15.3.1",
    "@playwright/test": "^1.52.0",
    "@swc-node/jest": "^1.8.13",
    "@swc/core": "^1.11.21",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",

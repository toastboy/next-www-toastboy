{
    "name": "next-www-toastboy",
    "private": true,
    "scripts": {
        "build:storybook": "storybook build",
        "build": "npm run generate && DEBUG=app:* next build",
        "clean": "rm -rf .next",
        "crawllivesite": "npx ts-node -r tsconfig-paths/register --project tsconfig.scripts.json --compiler-options '{\"module\":\"CommonJS\"}' src/lib/crawllivesite/crawllivesite.ts",
        "deepclean": "rm -rf .next node_modules package-lock.json && npm cache clean --force",
        "dev": "npm run generate && docker compose up -d && next dev",
        "generate": "prisma generate",
        "importlivedb": "TS_NODE_PROJECT=tsconfig.scripts.json TSX_TSCONFIG=tsconfig.scripts.json tsx -r tsconfig-paths/register src/lib/importlivedb/importlivedb.ts",
        "lint:fix": "eslint . --fix",
        "lint": "eslint .",
        "migrate": "prisma migrate dev",
        "start:ci": "next start --hostname 127.0.0.1 --port 3000",
        "start": "docker compose up -d && next start",
        "stop": "docker compose down",
        "storybook": "storybook dev -p 6006",
        "test:api:coverage": "COVERAGE=true jest --selectProjects api",
        "test:api": "jest --selectProjects api",
        "test:chromatic": "npx chromatic",
        "test:components:coverage": "COVERAGE=true  jest --selectProjects components",
        "test:components": "jest --selectProjects components",
        "test:coverage": "COVERAGE=true jest",
        "test:services:coverage": "COVERAGE=true jest --selectProjects services",
        "test:services": "jest --selectProjects services",
        "test:storybook:update-snapshots": "test-storybook --url http://127.0.0.1:6006 --updateSnapshot",
        "test:storybook": "test-storybook --url http://127.0.0.1:6006",
        "test:watch": "jest --watch",
        "test": "jest",
        "typecheck": "tsc --noEmit"
    },
